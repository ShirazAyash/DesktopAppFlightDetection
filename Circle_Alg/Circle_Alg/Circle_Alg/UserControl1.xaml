<UserControl x:Class="Circle_Alg.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Circle_Alg"
             xmlns:app="clr-namespace:Circle_Alg"
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <oxy:PlotView x:Name="graph1"  Model="{Binding VM_DllGraph, Mode=OneWay}"  Grid.Column="1" Grid.Row="1" >
        </oxy:PlotView>
    </Grid>
</UserControl>
